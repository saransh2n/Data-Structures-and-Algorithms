// here we are given an array consisting of 0's, 1's and 2's 
// we need to sort without using sort func and in o(n)

void sortarray (vector<int>& nums){
int i = 0; 
int j = 0;
int k = 0;

while ( i <= k ){
  if (nums[i] == 0){
    swap( nums[i],nums[j]);
    i++;
    j++;
  }
  else if (nums[i] == 2){
    swap( nums[i], nums[k];
    k--;
    }
    else{ i++;}
}

}
